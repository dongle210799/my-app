{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": [2],
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
    "sort-imports": [2, { "ignoreDeclarationSort": true, "allowSeparatedGroups": true }]
  },
  "extends": ["eslint:recommended", "next/core-web-vitals","prettier","plugin:@typescript-eslint/recommended","airbnb/hooks"]
}